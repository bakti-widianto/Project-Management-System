<body class="text-center">


  <form class="form-signin" action="/login" method="POST">
   
    <div class="card border-dark" style="width: 450px">

      <div class="card-body">
        <img class="mb-4" src="/images/owl-308773_640.png" alt="" width="80" height="80">
        <h1 class="h3 mb-3 font-weight-normal">Project Management System</h1>
        <h1 class="h3 mb-3 font-weight-normal">Sign In</h1>
        <label for="inputEmail" class="sr-only">Email address</label>
        <input type="email" id="inputEmail" name="email" class="form-control" placeholder="Email address" required=""
          autofocus="">
        <label for="inputPassword" class="sr-only">Password</label>
        <input type="password" id="inputPassword" name="password" class="form-control" placeholder="Password"
          required="">
        <div class="checkbox mb-3">
          <label>
            <input type="checkbox" value="remember-me"> Remember me
          </label>
        </div>
        <% if(loginInfo.length > 0) {%>
        <div class="alert alert-danger" role="alert">
          <%= loginInfo %>
        </div>
        <% } %>
        <button class="btn btn-lg btn-primary btn-block" type="submit">Sign in</button>
        <p class="mt-5 mb-3 text-muted">Â© 2020 Bakti Widianto</p>

      </div>
    </div>
  </form>


